<view class="page" data-theme="{{theme}}">
	<view class="page__hd">
		<view class="page__title">Progress</view>
		<view class="page__desc">进度条</view>
	</view>
	<view class="page__bd page__bd_spacing">
		<view class="progress">
			<view class="progress__bar">
				<view class="progress__inner-bar js_progress" style="width: 0%;"></view>
			</view>
			<a class="progress__opr">
				<i class="icon-cancel"></i>
			</a>
		</view>
		
		<view class="progress">
			<view class="progress__bar">
				<view class="progress__inner-bar js_progress" style="width: 50%;"></view>
			</view>
			<a class="progress__opr">
				<i class="icon-cancel"></i>
			</a>
		</view>
		
		<view class="progress">
			<view class="progress__bar">
				<view class="progress__inner-bar js_progress" style="width: 80%;"></view>
			</view>
			<a class="progress__opr">
				<i class="icon-cancel"></i>
			</a>
		</view>
		<view class="btn-area">
			<a class="btn btn_primary">上传</a>
		</view>
	</view>
</view>
