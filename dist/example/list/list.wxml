<view class="page" data-theme="{{theme}}">
    <view class="page__hd">
        <view class="page__title">List</view>
        <view class="page__desc">列表</view>
    </view>
    <view class="page__bd">
        <view class="cells__title">带说明的列表项</view>
        <view class="cells cells_after-title">
            <view class="cell">
                <view class="cell__bd">标题文字</view>
                <view class="cell__ft">说明文字</view>
            </view>
        </view>

        <view class="cells__title">带图标、说明的列表项</view>
        <view class="cells cells_after-title">
            <view class="cell cell_example ">
                <view class="cell__hd">
                    <image src="{{icon}}" style="margin-right: 16px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="cell__bd">标题文字</view>
                <view class="cell__ft">说明文字</view>
            </view>
            <view class="cell cell_example ">
                <view class="cell__hd">
                    <image src="{{icon}}" style="margin-right: 16px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="cell__bd">标题文字</view>
                <view class="cell__ft">说明文字</view>
            </view>
        </view>

        <view class="cells__title">带跳转的列表项</view>
        <view class="cells cells_after-title">
            <navigator url="" class="cell cell_access" hover-class="cell_active">
                <view class="cell__bd">cell standard</view>
                <view class="cell__ft cell__ft_in-access"></view>
            </navigator>
            <navigator url="" class="cell cell_access" hover-class="cell_active">
                <view class="cell__bd">cell standard</view>
                <view class="cell__ft cell__ft_in-access"></view>
            </navigator>
        </view>

        <view class="cells__title">带说明、跳转的列表项</view>
        <view class="cells cells_after-title">
            <navigator url="" class="cell cell_access" hover-class="cell_active">
                <view class="cell__bd">cell standard</view>
                <view class="cell__ft cell__ft_in-access">说明文字</view>
            </navigator>
            <navigator url="" class="cell cell_access" hover-class="cell_active">
                <view class="cell__bd">cell standard</view>
                <view class="cell__ft cell__ft_in-access">说明文字</view>
            </navigator>
        </view>

        <view class="cells__title">带图标、说明、跳转的列表项</view>
        <view class="cells cells_after-title">
            <navigator url="" class="cell cell_access" hover-class="cell_active">
                <view class="cell__hd">
                    <image src="{{icon}}" style="margin-right: 16px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="cell__bd">cell standard</view>
                <view class="cell__ft cell__ft_in-access">说明文字</view>
            </navigator>
            <navigator url="" class="cell cell_example cell_access" hover-class="cell_active">
                <view class="cell__hd">
                    <image src="{{icon}}" style="margin-right: 16px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="cell__bd">cell standard</view>
                <view class="cell__ft cell__ft_in-access">说明文字</view>
            </navigator>
        </view>
    </view>
</view>
